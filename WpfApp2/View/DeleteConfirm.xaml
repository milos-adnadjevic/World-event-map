<Window x:Class="WpfApp2.View.DeleteConfirm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2.View"
        mc:Ignorable="d"
        Title="Delete confirmation" Height="150" Width="200">
    <Grid>
        <Label Content="Are you sure?"  Margin="50,30,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" FontSize="15" />
        <Button Content="Yes" HorizontalAlignment="Left" Margin="120,70,0,0" VerticalAlignment="Top" Width="30" Click="Yes_Click"   Name="Yes"/>
        <Button Content="No" HorizontalAlignment="Left" Margin="50,70,0,0" VerticalAlignment="Top" Width="30" Click="No_Click"   Name="No" Focusable="True"/>


    </Grid>
</Window>
